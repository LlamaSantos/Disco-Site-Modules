<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html>
<head>
    <link rel="stylesheet/less" type="text/css" href="style.less" />
    <script type="text/javascript" src="less-1.3.0.min.js"></script>
</head>
<body>
    <div class="containers-wrapper">
        <div class="cards-container"></div>
        <div class="boxes-container">
            <div id="apples" class="box"></div>
            <div id="oranges" class="box"></div>
        </div>
    </div>

    <script id="card-template" type="text/template">
        <div class="card" fullName="{{fullName}}" fbId="{{fbId}}" obj="{{jsonObj}}">
            <div class="general-info">
                <div class="fb-image-container">
                    <img class="fb-image" src="https://graph.facebook.com/{{fbId}}/picture?type=normal" />
                </div>
                <div class="first-name">{{firstName}}</div>
                <div class="last-name">{{lastName}}</div>
                <div class="dob">{{dob}}</div>
            </div>
            <div class="bio">{{bio}}</div>
        </div>
    </script>

    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.18/jquery-ui.min.js"></script>
    <script type="text/javascript" src="json2.js"></script>
    <script type="text/javascript" src="underscore-min.js"></script>
    <script type="text/javascript" src="mustache.js"></script>
    <script type="text/javascript" src="main.js"></script>
</body>
</html>